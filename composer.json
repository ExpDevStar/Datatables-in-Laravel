{
    "name": "yajra/laravel-datatables-oracle",
    "description": "jQuery DataTables API for Laravel 4|5",
    "keywords" : ["laravel4","laravel5","laravel","datatables","datatable","datatables jquery plugin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/view": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/filesystem": "~5.0",
        "laravelcollective/html": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
      "league/fractal": "0.12.* for transforming of ajax response",
      "maatwebsite/excel": "^2.0 for DataTables server-side buttons implementation.",
      "dompdf/dompdf": "^0.6.1 for exporting to PDF files."
    },
    "autoload": {
        "psr-0": {
            "yajra\\Datatables": "src/"
        }
    },
    "minimum-stability": "stable"
}
